{
  "name": "nyckhub",
  "version": "1.0.0",
  "description": "Multiple media platform combiner.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "zWolfy",
  "devDependencies": {
    "electron-builder": "^22.9.1"
  },
  "build": {
    "appId": "tech.zwolfy.nyckhub",
    "productName": "NyckHub",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.png"
    },
    "linux": {
      "target": "appimage",
      "icon": "build/icons/256x256.png"
    },
    "artifactName": "NyckHub Setup.${ext}"
  },
  "dependencies": {
    "discord-rich-presence": "0.0.8",
    "electron": "^11.2.1",
    "fs": "0.0.1-security",
    "youtube-info": "^1.3.2"
  }
}
